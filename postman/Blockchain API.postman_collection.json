		{
			"name": "Get Transaction for Address",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var jsonBody= pm.response.json();",
							"",
							"console.log('Transactions count: ', jsonBody.transactions.length)"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "noauth"
				},
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/{{api_v}}/eth/address/:address/transactions?from=9221003",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"{{api_v}}",
						"eth",
						"address",
						":address",
						"transactions"
					],
					"query": [
						{
							"key": "from",
							"value": "9221003",
							"description": "The starting block. Default is 0 or contract creation block"
						},
						{
							"key": "to",
							"value": "2000000",
							"description": "The end block. Default is the latest block on the chain",
							"disabled": true
						},
						{
							"key": "limit",
							"value": "500",
							"description": "Limits the number of results. Default is 1000. Max is 1000",
							"disabled": true
						},
						{
							"key": "order",
							"value": "asc",
							"description": "Selecting the order of transactions by block number. Options are: asc, desc. Default is asc",
							"disabled": true
						},
						{
							"key": "page",
							"value": "1",
							"description": "Number of page. Default is 1",
							"disabled": true
						}
					],
					"variable": [
						{
							"key": "address",
							"value": "0x7b79995e5f793a07bc00c21412e50ecae098e7f9"
						}
					]
				}
			},
			"response": []
